<?xml version="1.0" encoding="utf-8" ?>

<!-- Packet definition from the server -->

<Definition namespace="CT.Packets">
	<!-- Using Segments -->>
	<using>CT.Packets</using>>
	<using>CT.Common.DataType</using>>
	<using>CT.Common.Serialization</using>>
	<using>CT.Common.Serialization.Type</using>>

	<!-- Declare Data Types -->>
	<struct name="NetVec2">
		<Single name="X"/>
		<Single name="Y"/>
	</struct>
	
	<struct name="NetVecZ">
		<Single name="Z"/>
	</struct>
	
	<class name="NetTransform">
		<NetVec2 name="Position"/>
		<NetVecZ name="PositionZ"/>
		<NetVec2 name="Velocity"/>
	</class>

	<!-- Declare Packets -->>
</Definition>