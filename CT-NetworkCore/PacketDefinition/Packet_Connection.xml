<?xml version="1.0" encoding="utf-8" ?>

<!-- Packet definition from the server -->

<Definition namespace="CT.Packets">
	<!-- Using Segments -->>
	<using>CT.Packets</using>>
	<using>CT.Network.DataType</using>>
	<using>CT.Network.Serialization</using>>
	<using>CT.Network.Serialization.Type</using>>
	
	<!-- Declare Data Types -->>
	<class name="UserProfile">
		<NetStringShort name="Clothes"/>
	</class>
	
	<!-- Declare Packets -->>
	<client name="TryConnect">
		<UInt64 name="Id"/>
		<NetStringShort name="Token"/>
		<MatchEndpoint name="MatchTo"/>
	</client>

	<server name="AckConnect">
		<ConnectAck name="Result"/>
	</server>

	<client name="TrySendUserProfile">
		<UserProfile name="UserProfile"/>
	</client>

	<server name="InitialWorldState">
		<NetStringShort name="MiniGameName"/>
	</server>
	
	<server name="SpawnEntities">
		<NetArray name="SpawnedEntities" type="EntityPlayerState"/>
	</server>

	<server name="DespawnEntities">
		<NetFixedArray name="DespawnEntities" type="NetEntityID"/>
	</server>
</Definition>